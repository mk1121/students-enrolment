# Production Environment Variables
# Copy this file to .env.production and update with your actual values

# Backend API URL for production
REACT_APP_API_URL=https://your-production-backend-url.onrender.com

# Stripe Publishable Key (Live Mode)
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_live_your_stripe_live_publishable_key_here

# Environment identifier
REACT_APP_ENVIRONMENT=production

# Optional: Disable debug features in production
REACT_APP_DEBUG_MODE=false

# Optional: API timeout in milliseconds
REACT_APP_API_TIMEOUT=5000
