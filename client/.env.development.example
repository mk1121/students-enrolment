# Development/Staging Environment Variables
# Copy this file to .env.development and update with your actual values

# Backend API URL for development/staging
REACT_APP_API_URL=https://your-staging-backend-url.onrender.com

# Stripe Publishable Key (Test Mode)
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_test_publishable_key_here

# Environment identifier
REACT_APP_ENVIRONMENT=development

# Optional: Enable development features
REACT_APP_DEBUG_MODE=true

# Optional: API timeout in milliseconds
REACT_APP_API_TIMEOUT=10000
